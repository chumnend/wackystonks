{
  "name": "ws-socket-server",
  "version": "1.2.0",
  "description": "Socket express server logic",
  "main": "dist/index.js",
  "author": "Nicholas Chumney",
  "license": "ISC",
  "scripts": {
    "build": "tsc",
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "test": "nyc ./node_modules/.bin/_mocha -r ts-node/register tests/**/*.test.ts",
