{
  "name": "ws-server",
  "version": "0.5.0",
  "description": "wacky stonks express server",
  "main": "dist/index.js",
  "author": "Nicholas Chumney",
  "license": "ISC",
  "scripts": {
    "prestart": "yarn build",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts",
    "build": "yarn codegen && tsc && cp -r src/graphql/schemas dist/graphql",
    "test": "./node_modules/.bin/_mocha -r ts-node/register tests/**/*.test.ts",
    "coverage": "nyc yarn test",
    "lint": "./node_modules/.bin/eslint ./src",
    "lint:fix": "./node_modules/.bin/eslint ./src --fix && ./node_modules/.bin/prettier --write ./src",
    "codegen": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "apollo-server-express": "^3.1.2",
    "bcrypt": "^5.0.1",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "graphql": "^15.5.1",
    "graphql-import-node": "^0.0.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "pg": "^8.7.1",
    "sequelize": "^6.6.5",
    "socket.io": "^4.2.0",
    "ws-assets": "^0.8.0"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "2.0.1",
    "@graphql-codegen/typescript": "2.0.0",
    "@graphql-codegen/typescript-resolvers": "2.0.0",
    "@graphql-tools/schema": "^8.0.3",
    "@graphql-tools/utils": "^8.0.2",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.2.21",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.172",
    "@types/node": "^16.4.13",
    "@types/pg": "^8.6.1",
    "@types/socket.io": "^3.0.2",
    "@types/validator": "^13.6.3",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "@typescript-eslint/parser": "^4.29.0",
    "chai": "^4.3.4",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "mocha": "^9.1.1",
    "nodemon": "^2.0.12",
    "nyc": "^15.1.0",
    "prettier": "^2.3.2",
    "ts-node": "^10.1.0",
    "typescript": "^4.3.5"
  }
}
